				<color rgba="0 0 0 1"/>  
			</material>  
		</visual>  
	</link>  


	<joint name="behind_left_wheel_joint" type="continuous">
		<axis xyz="0 0 1"/>
		<parent link="base_link"/>
		<child link="behind_left_wheel"/>
		<origin rpy="0 1.57075 0" xyz="-0.1013 -0.0585 -0.024"/>  
		<joint_properties damping="0.0" friction="0.0"/>
	</joint>

